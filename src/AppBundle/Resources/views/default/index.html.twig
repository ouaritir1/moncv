                        <div class="col-md-6">
                                <div class="content-item">
                                    <small>{{ experience.getDebut()}} - {{ experience.getFin() }}</small>
                                    <h3>{{ experience.getName() }}</h3>
                                    <h4>{{ experience.getPlace() }}</h4>
                                    <a href="{{ path('edit_experience', {'id': experience.getId()}) }}">edit experience</a>
                                    <a href="{{ path('delete_experience', {'id': experience.getId()}) }}">
